    </div>
  </div>


  <% if false %>
    <% CousinPage.all.each do |cousin| %>
      <div class="cousin">
        <h2><%= cousin.name %></h2>
        <%= cousin.parents %>
        <%= cousin.children %>
        <%= cousin.news_brief %>
        <a href="<%= cousin.request_path %>">More news...</a>
      </div>
    <% end %>
  <% end %>

